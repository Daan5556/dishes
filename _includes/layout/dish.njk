---
layout: layout/base.njk
---

<div class="banner dish">
  <a href="{{ page.url }}"><h2>{{ country }}</h2></a>

  <p>{{ dish }}</p>
  <time datetime="{{ isoDate }}">{{ formattedDate }}</time>

  <img src="{{ image }}" fetchpriority="high" width="400" alt="{{ dish }}" />
</div>

{{ content | safe }}
